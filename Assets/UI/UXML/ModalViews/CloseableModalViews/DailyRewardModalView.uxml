        <ui:Button name="daily-reward__claim-reward-button" enable-rich-text="false" text="CLAIM REWARD" parse-escape-sequences="true" display-tooltip-when-elided="true" style="margin: 2% 0; -unity-text-align: middle-left; font-size: 40px; -unity-font-style: bold;" />
