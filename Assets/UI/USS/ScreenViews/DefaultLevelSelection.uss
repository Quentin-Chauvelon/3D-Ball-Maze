.unity-scroll-view__content-container {
    flex-direction: column;
    flex-wrap: wrap;
    justify-content: center;
}

.level-selection-item {
    width: 300px;
    height: 300px;
    margin-top: 20px;
    margin-right: 20px;
    margin-bottom: 20px;
    margin-left: 20px;
}

.level-selection-item-locked #default-level-selection-template__container-button {
    display: none;
}

.level-selection-item-unlocked #default-level-selection-template__lock-container {
    display: none;
}